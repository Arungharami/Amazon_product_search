    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "abf93484",
        "outputId": "820724bc-935b-4195-8edc-b09435af3367"
      },
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Assumes df_outcomes is available from the temporal splitting step.\n",
        "# If not, recreate it for self-contained execution:\n",
        "if 'df_outcomes' not in locals():\n",
        "    # This block is for self-contained execution if df_outcomes is not yet defined\n",
        "    num_customers = 120000\n",
        "    df = pd.DataFrame({\n",
        "        'customer_id': np.arange(1, num_customers + 1),\n",
        "        'Product_Uptake_M19_24': np.random.choice([0, 1], num_customers, p=[0.7, 0.3]),\n",
        "        'Churn_M19_24': np.random.choice([0, 1], num_customers, p=[0.9, 0.1]),\n",
        "        'Claim_Propensity_M19_24': np.random.choice([0, 1], num_customers, p=[0.85, 0.15]),\n",
        "        'Fraud_Risk_M19_24': np.random.choice([0, 1], num_customers, p=[0.99, 0.01])\n",
        "    })\n",
        "    df_outcomes = df[['customer_id', 'Product_Uptake_M19_24', 'Churn_M19_24', 'Claim_Propensity_M19_24', 'Fraud_Risk_M19_24']].copy()\n",
        "\n",
        "outcome_stats = []\n",
        "\n",
        "outcome_columns = [\n",
        "    'Product_Uptake_M19_24',\n",
        "    'Churn_M19_24',\n",
        "    'Claim_Propensity_M19_24',\n",
        "    'Fraud_Risk_M19_24'\n",
        "]\n",
        "\n",
        "for col in outcome_columns:\n",
        "    total_customers = len(df_outcomes)\n",
        "    positive_cases = df_outcomes[col].sum()\n",
        "    positive_rate = (positive_cases / total_customers) * 100\n",
        "\n",
        "    outcome_stats.append({\n",
        "        'Outcome': col.replace('_M19_24', '').replace('_', ' '), # Clean up name for display\n",
        "        'Total Customers': total_customers,\n",
        "        'Positive Cases': positive_cases,\n",
        "        'Positive Rate (%)': round(positive_rate, 2)\n",
        "    })\n",
        "\n",
        "df_outcome_stats = pd.DataFrame(outcome_stats)\n",
        "\n",
        "print(\"Comprehensive Summary Table (Table 1):\")\n",
        "print(df_outcome_stats)"
      ],
      "execution_count": 28,
      "outputs": [
