      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Assumes df_outcome_stats is available from the summary table creation step.\n",
        "# If not, recreate it for self-contained execution:\n",
        "if 'df_outcome_stats' not in locals():\n",
        "    # This block is for self-contained execution if df_outcome_stats is not yet defined\n",
        "    num_customers = 120000\n",
        "    df_outcomes_temp = pd.DataFrame({\n",
        "        'customer_id': np.arange(1, num_customers + 1),\n",
        "        'Product_Uptake_M19_24': np.random.choice([0, 1], num_customers, p=[0.7, 0.3]),\n",
        "        'Churn_M19_24': np.random.choice([0, 1], num_customers, p=[0.9, 0.1]),\n",
        "        'Claim_Propensity_M19_24': np.random.choice([0, 1], num_customers, p=[0.85, 0.15]),\n",
        "        'Fraud_Risk_M19_24': np.random.choice([0, 1], num_customers, p=[0.99, 0.01])\n",
        "    })\n",
        "\n",
        "    outcome_stats_temp = []\n",
        "    outcome_columns_temp = [\n",
        "        'Product_Uptake_M19_24',\n",
        "        'Churn_M19_24',\n",
        "        'Claim_Propensity_M19_24',\n",
        "        'Fraud_Risk_M19_24'\n",
        "    ]\n",
        "    for col in outcome_columns_temp:\n",
        "        total_customers = len(df_outcomes_temp)\n",
        "        positive_cases = df_outcomes_temp[col].sum()\n",
        "        positive_rate = (positive_cases / total_customers) * 100\n",
        "        outcome_stats_temp.append({\n",
        "            'Outcome': col.replace('_M19_24', '').replace('_', ' '),\n",
        "            'Total Customers': total_customers,\n",
        "            'Positive Cases': positive_cases,\n",
        "            'Positive Rate (%)': round(positive_rate, 2)\n",
        "        })\n",
        "    df_outcome_stats = pd.DataFrame(outcome_stats_temp)\n",
        "\n",
        "\n",
        "# Set a style for better aesthetics\n",
        "sns.set_style(\"whitegrid\")\n",
        "\n",
        "# Create the bar plot\n",
        "plt.figure(figsize=(10, 6))\n",
        "barplot = sns.barplot(x='Outcome', y='Positive Rate (%)', data=df_outcome_stats, hue='Outcome', palette='viridis', legend=False)\n",
        "\n",
        "# Add title and labels\n",
        "plt.title('Figure 1: Positive Rates for Each Outcome', fontsize=16)\n",
        "plt.xlabel('Outcome', fontsize=12)\n",
        "plt.ylabel('Positive Rate (%)', fontsize=12)\n",
        "\n",
        "# Rotate x-axis labels for better readability if needed\n",
        "plt.xticks(rotation=45, ha='right')\n",
        "\n",
        "# Add the exact positive rate value on top of each bar\n",
        "for index, row in df_outcome_stats.iterrows():\n",
        "    barplot.text(index, row['Positive Rate (%)'] + 0.5, f\"{row['Positive Rate (%)']:.2f}%\", color='black', ha=\"center\")\n",
        "\n",
        "# Adjust layout to prevent labels from overlapping\n",
        "plt.tight_layout()\n",
        "\n",
        "# Display the plot\n",
        "plt.show()\n",
        "\n",
        "display(plt.gcf())"
      ],
