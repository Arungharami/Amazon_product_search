            "\n",
            "   digital_to_total_interactions_ratio  \n",
            "0                             0.500000  \n",
            "1                             0.892857  \n",
            "2                             0.000000  \n",
            "3                             0.822785  \n",
            "4                             0.753086  \n",
            "\n",
            "Updated numeric_feature_columns:\n",
            "['Age', 'Income', 'deposits_M1_18', 'withdrawals_M1_18', 'card_spend_M1_18', 'loans_M1_18', 'digital_logins_M1_18', 'branch_visits_M1_18', 'call_center_contacts_M1_18', 'spend_to_deposit_ratio', 'withdrawal_to_deposit_ratio', 'digital_to_total_interactions_ratio']\n"
